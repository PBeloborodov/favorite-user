{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
     "jsx": "react-native",
     "baseUrl": "./",
     "paths": {
       "@components/*": ["src/components/*"],
       "@screens/*": ["src/screens/*"],
       "@store/*": ["src/store/*"],
       "@hooks/*": ["src/hooks/*"],
       "@assets/*": ["src/assets/*"],
       "@types/*": ["src/types/*"]
     }
  }
}
